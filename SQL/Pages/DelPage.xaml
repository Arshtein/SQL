<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Title="Удаление"
             x:Class="SQL.Pages.DelPage">
    
    <StackLayout>
        <Editor x:Name="Text" FontSize="25" AutoSize="TextChanges" IsEnabled="False"/>
        <Button x:Name="Del" FontSize="25" Text="Удалить" VerticalOptions="EndAndExpand" IsEnabled="False" Clicked="dbClick"/>
    </StackLayout>
</ContentPage>