        <Editor x:Name="Text" FontSize="25" AutoSize="TextChanges" IsEnabled="False"/>
        <Button x:Name="Upd" Font="25" Text="Сохранить" VerticalOptions="EndAndExpand" IsEnabled="False" Clicked="udClick"/>
    </StackLayout>
</ContentPage>